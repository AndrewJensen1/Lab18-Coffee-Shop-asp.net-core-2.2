
@{
    ViewData["Title"] = "AddUserToList";
}

<h1>AddUserToList</h1>


@model IEnumerable<Lab18_Coffee_Shop.Models.RegisterUser>

<table class="table table-bordered table-striped table-hover">
    <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
        </tr>

    }
</table>


